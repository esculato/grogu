{
  "extends": [
    "config:recommended"
  ],
  "prHourlyLimit": 5, // Increase the limit, e.g., from the default of 2 to 5
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true,
      "automergeType": "pr"
    }
  ]
}
